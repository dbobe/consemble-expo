{
  "expo": {
    "name": "consemble-expo",
    "slug": "consemble-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Timber.png",
    "scheme": "consembleexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.consemble.consembleexpo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.consemble.consembleexpo"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/Timber.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/CabinetGrotesk-Variable.ttf",
            "./src/assets/fonts/Gantari-VariableFont_wght.ttf"
          ]
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0530ce24-003b-4f0c-9c95-d029f3f5f7db"
      }
    }
  }
}
